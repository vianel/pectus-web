			    <button width="75px" height="50px" sclass="btn-success">
          		 	<attribute name="onClick" onClick="@command('cancelar')">						
						    dialogoColaboracionEvento.setVisible(false);						  	
			</attribute>
			Salir</button>
			
			</div>

</div>
  
  
  </tabpanel>
 
 
			  
  </tabpanels>


 </tabbox>
    </vlayout>
</window>








