        <label value="@load(vm2.eventoSelected.lugar.direccion)"></label>
        </div>
        </div>
    <tabbox >
    
    <tabs>
      <tab style="border: 2px solid #f7aff3;border-bottom: 0;" label="Registrar Colaboracion"/>
