			 <attribute name="onClick">						
						    dialogoColaboracionEvento.setVisible(false);						  	
			</attribute>Salir</button>
		</div>
	
	<!-- Datos de la cita-->
			
	
 
 
  </tabpanel>
  
  <tabpanel>
  	
