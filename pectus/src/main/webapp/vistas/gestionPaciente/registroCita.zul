 <a label="Citas" iconSclass="z-icon-group" style="color:#6FD7BB;"/>
  <tabbox >
    <tabs>

      <tab style="border: 2px solid #f7aff3;border-bottom: 0;" label="Citas"/>
    
    </tabs>
    <tabpanels>
		<tabpanel >
		 <div apply="org.zkoss.bind.BindComposer"
			viewModel="@id('vmc') @init('com.ucla.frontend.pectus.controllers.FilterViewModel')">
			
<!-- 	<div apply="com.ucla.frontend.pectus.controllers.Dialogo">		 -->
		<button id="btnEditarPaciente" sclass="btn-success" onClick="@command('abrirDialogoRegistrarCita')" >NUEVO</button>
<!-- 	</div> -->
	
	<grid height="400px"  pagingPosition="bottom" mold="paging" pageSize="7" model="@load(vmc.foodModelCita)">
		<auxhead>
            <auxheader label="CITAS" colspan="6" />
        </auxhead>
        
        <auxhead sclass="category-center">
	        <auxheader colspan="1">
	            <textbox instant="true" width="100px" 
	                value="@bind(vmc.citaFilter.nombrePaciente)" onChange="@command('changeFilterCita')" />
	        </auxheader>
			
			<auxheader colspan="1">
                <textbox instant="true"  width="100px"
                    value="@bind(vmc.citaFilter.nombreClinica)" onChange="@command('changeFilterCita')"/>
            </auxheader>
            
            <auxheader colspan="1">
                <textbox instant="true" width="100px"
                    value="@bind(vmc.citaFilter.nombreEstudio)" onChange="@command('changeFilterCita')"/>
            </auxheader>
            
            <auxheader label="Detalles" colspan="3" />
        
        </auxhead>
	
	<columns>
            <column hflex="1">Paciente</column>
            <column hflex="1">Clinica</column>
            <column hflex="1">Estudio</column>
            <column hflex="1">Fecha</column>
            <column hflex="1">Hora</column>
            <column hflex="1">Editar</column>
        </columns>
        <template name="model">
            <row>
                <label value="@load(each.paciente.nombre)" />
                <label value="@load(each.clinica.nombre)" />
                <label value="@load(each.estudio.nombre)" />
                <label value="@load(each.fecha)"/>
                <label value="@load(each.hora)"/>
               	<button id="" sclass="btn-success" onClick="@command('abrirDialogoRegistrarPaciente')">Editar</button>
            </row>
        </template>
        <foot>
            <footer span="6"  label="asdasd" />
        </foot>
	
	</grid>			
</div>
		
		</tabpanel>
		
    </tabpanels>
  </tabbox>

</zk>