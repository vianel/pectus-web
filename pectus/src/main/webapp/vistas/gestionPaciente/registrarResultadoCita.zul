<zk xmlns:n="native">
 <a label="Citas" iconSclass="z-icon-group" style="color:#6FD7BB;"/>
  <tabbox >
    <tabs>
      	<tab style="border: 2px solid #f7aff3;border-bottom: 0;" label="Citas terminadas"/>
