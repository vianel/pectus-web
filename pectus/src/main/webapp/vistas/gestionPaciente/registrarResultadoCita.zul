<!-- 			        </auxheader> -->
<!-- 					<auxheader colspan="1"> -->
<!-- 		                <textbox instant="true"  width="100px" -->
<!-- 		                    value="@bind(vm.citaFilter.clinica)" onChange="@command('changeFilter')"/> -->
<!-- 		            </auxheader> -->
<!-- 		            <auxheader colspan="1"> -->
